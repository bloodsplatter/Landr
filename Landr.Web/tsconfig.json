{
  "compilerOptions": {
    "noImplicitAny": false,
    "allowJs": true,
    "jsx": "react",
    "module": "ES6",
    "noEmitOnError": true,
    "removeComments": false,
    "outDir": "wwwroot/js/",
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "scripts/**/*"
  ]
}
